{
	"name": "gupshup-whatsapp-sdk",
	"version": "0.0.1",
	"description": "An unofficial library to help with the use of GupShup WhatsApp APIs",
	"main": "dist/gupshup",
	"typings": "dist/gupshup",
	"scripts": {
		"clean": "rm -rf dist && mkdir dist",
		"cp-ts": "cp lib/gupshup.d.ts dist/",
		"build:commonjs": "tsc",
		"build": "npm run clean && npm run build:commonjs && npm run cp-ts",
		"debug": "npm run build && node-debug examples/index.js"
	},
	"author": "Shashank Shekhar <shashank7200@gmail.com>",
	"license": "MIT",
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/ashwamegh/gupshup-whatsapp-sdk.git"
	},
	"devDependencies": {
		"@types/node": "^14.14.21",
		"babel-cli": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.26.0"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"querystring": "^0.2.0",
		"typescript": "^4.1.3"
	}
}
